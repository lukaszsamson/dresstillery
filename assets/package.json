{
  "author": "Lukasz Samson",
  "name": "dresstillery",
  "version": "0.1.0",
  "description": "Dress shop in elm",
  "scripts": {
    "deploy": "BACKEND_URL=/api webpack -p --config webpack.config.phoenix.js",
    "watch": "BACKEND_URL=/api webpack --watch --progress --color --config webpack.config.phoenix.js",
    "api": "json-server -w -p $API_PORT db.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo $PORT && webpack-dev-server --hot --inline --port $PORT --host $HOST",
    "build": "webpack",
    "postinstall": "elm-package install -y",
    "start": "nf start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukaszsamson/dresstilery"
  },
  "license": "MIT",
  "devDependencies": {
    "bootstrap-loader": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.3.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "json-server": "^0.11.1",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "bootstrap": "^4.0.0-alpha.6",
    "qrcode-js": "0.0.2"
  }
}
